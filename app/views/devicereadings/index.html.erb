<div class="page-header">
  <h1>
    Devicereadings
    <a href="/devicereadings/new" class="btn btn-lg btn-success">New Devicereading</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Lat</th>
        <th>Lng</th>
        <th>Tmp</th>
        <th>Hum</th>
        <th>Fix</th>
        <th>Satellites</th>
        <th>Gpstime</th>
        <th>Batt</th>
        <th>Device</th>
        <th>Actions</th>
      </tr>

      <% @devicereadings.each do |devicereading| %>
      <tr>
        <td><%= devicereading.lat %></td>
        <td><%= devicereading.lng %></td>
        <td><%= devicereading.tmp %></td>
        <td><%= devicereading.hum %></td>
        <td><%= devicereading.fix %></td>
        <td><%= devicereading.satellites %></td>
        <td><%= devicereading.gpstime %></td>
        <td><%= devicereading.batt %></td>
        <td><%= devicereading.device_id %></td>
        <td>
          <a href="/devicereadings/<%= devicereading.id %>" class="btn btn-primary">Show</a>
          <a href="/devicereadings/<%= devicereading.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_devicereading/<%= devicereading.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
